{
  "autoload": {
    // include PHP classes without having to use statements like require and include
    "psr-4": {
      "Core\\": "core/",
      "Model\\": "model/",
      "App\\": "app/",
      "Config\\": "config/",
      "Helper\\": "helper/"
    },
    "files": [
      "helper/common_helper.php"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "5.6.*"
  },
  "require": {
    "guzzlehttp/guzzle": "^6.2",
    "illuminate/database": "^5.3"
  }
}
